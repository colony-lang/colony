Int: type = u8 | i32 | u32 | i64 | u64
Float: type = f32 | f64
Number: type = int | float

A: type = <U := Number> -> (
    x: U,
    y: U,
)

B: type = <V := Number> -> (
    u: V,
    v: V,
)

C: type = <T := Number> -> (
    w: T,
)

D: type = <T := Number> -> (
    c: C<T>,
)

E: type = <T := Number> -> (
    a: A<T>,
    b: B<T>,
    d: D<T>,
)

/// ....
e: E = <i64>(
    a = <i64>
    x = 1,
    y = 2,
)
