//
// struct
//
A: struct = (
	x: f32 = 0.0,
	y: f32 = 0.0,
)

//
a0: A = A(1.0, 2.0)
r := a0.x + a0.y

//
a1 := A(1.0, 2.0)
r = a1.x + a1.y

//
A(x, y) := a0
r := x + y

//
// list
//
b: list = [0.0, 1.0]
r := b[0] + b[1]

(x, y) := b
r := x + y

//
// dict
//
d: dict = {'x': 0.0, 'y': 1.1}
r := d['x'] + d.y

{x, y} := d
r := x + y

