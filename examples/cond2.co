/* example 1 */
let s0: i32 = sum(
    range(10)
        .map(fn (n: i32) -> i32: n * 2)
)

let s0_a: i32 = sum(
    map(
        range(10),
        fn (n: i32) -> i32: n * 2
    )
)

/* example 2 */
let s1: i32 = sum(
    range(10)
        .filter(fn (n: i32) -> i32: n < 5)
        .map(fn (n: i32) -> i32: n * 2
    )
)

let s1_0: i32 = sum(
    map(
        filter(
            range(10),
            fn (n: i32) -> i32: n < 5
        ),
        fn (n: i32) -> i32: n * 2
    )
)
