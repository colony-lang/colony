generics = ([types]) ->

//
// example
//
A = generic[K, V] type {
    .__gentypes__: [K, V],
    .items: dict[K, V],
    
    .__init__: (self) -> Self {
        self.items: dict[K, V]
        self
    },
    
    .set: (self, key: K, value: V) -> {
        self.items[key] = value
        self
    },
}

